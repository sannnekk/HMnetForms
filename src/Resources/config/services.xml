<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>

        <service id="HMnet\Forms\Core\Content\Form\FormEntity">
            <tag name="shopware.entity"/>
        </service>

        <service id="HMnet\Forms\Core\Content\FormField\FormFieldEntity">
            <tag name="shopware.entity"/>
        </service>

        <service id="HMnet\Forms\Core\Content\FormSubmission\FormSubmissionEntity">
            <tag name="shopware.entity"/>
        </service>

        <service id="HMnet\Forms\Content\Cms\HMnetFormCmsElementResolver">
            <tag name="shopware.cms.data_resolver" />
        </service>

        <service id="HMnet\Forms\Controller\HMnetFormController" public="true">
            <argument type="service" id="hmnet_form.repository"/>
            <argument type="service" id="hmnet_form_submission.repository"/>
            <call method="setContainer">
                <argument type="service" id="service_container"/>
            </call>
            <tag name="controller.service_arguments"/>
        </service>

    </services>
</container>